{
  "Logging": {
    "OutputTemplate": "{Timestamp:o} {RequestId,13} [{Level:u3}] {Message} Properties: {Properties:j} ({EventId:x8}){NewLine}{Exception}",
    "PathFormat": "c:\\tmp\\logs\\Petroineos.PowerPosition-{Date}.log",
    "MinimumLevel": "Information",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "SchedulingInterval": 2,
  "OutputPath": "c:\\tmp\\output"
}

